{
  "name": "meg",
  "version": "0.0.0",
  "description": "Meta language + PEG grammar = Any language",
  "keywords": [
    "meta",
    "peg",
    "grammar",
    "macro",
    "pre-processor",
    "jit",
    "javascript",
    "c",
    "generator",
    "ecmascript",
    "nodejs"
  ],
  "homepage": "https://github.com/futagoza/meg",
  "repository": "futagoza/meg",
  "bugs": "https://github.com/futagoza/meg/issues",
  "license": "MIT",
  "author": "Futago-za Ryuu <futagoza.ryuu@gmail.com>",

  "main": "index.js",
  "browser": "dist/meg-umd-es5.js",
  "jsnext:main": "dist/meg-src-es6.js",
  "bin": "bin/meg",
  "scripts": {
    "lint": "eslint lib test",
    "clean": "rimraf dist/*.js",
    "prebuild": "npm run lint && npm run clean",
    "build": "npm run build:es5 && npm run build:es6",
    "build:es5": "rollup --environment es5 -c",
    "build:es6": "rollup --environment es6 -c",
    "watch": "npm run watch:es5 && npm run watch:es6",
    "watch:es5": "rollup -w --environment es5 -c",
    "watch:es6": "rollup -w --environment es6 -c",
    "test": "mocha --compilers js:babel-register",
    "test:es6": "mocha",
    "prepublish": "npm run lint && npm test"
  },
  "devDependencies": {
    "eslint": "3.17.1",
    "babel-eslint": "7.1.1",
    "rimraf": "2.6.1",
    "babel-register": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "rollup": "0.41.5",
    "rollup-plugin-json": "2.1.0",
    "rollup-plugin-babel": "2.7.1",
    "babelrc-rollup": "3.0.0",
    "babel-preset-es2015-rollup": "3.0.0",
    "mocha": "3.2.0",
    "pretty-hrtime": "1.0.3"
  },
  "engines": {
    "node": ">= 4.0.0"
  }
}
